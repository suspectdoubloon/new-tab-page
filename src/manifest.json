{
    "name": "Simple New Tab",
    "version": "1.0",
    "description": "Overwrite MS Edge new tab",
    "permissions": ["topSites"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_title": "Simple New Tab Page"
    },
    "manifest_version": 2
}